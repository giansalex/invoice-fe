{% extends 'base.html.twig' %}

{% block body %}
    <h1>Usertax</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ userTax.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ userTax.code }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ userTax.userId }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipo-tributo_index') }}">Back to the list</a>
